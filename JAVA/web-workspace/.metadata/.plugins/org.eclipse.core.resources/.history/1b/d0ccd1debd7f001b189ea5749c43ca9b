<title>Addition</title>
</head>
<body>
	<!-- % : JSP 프로그램 -->
	<%
		int num1 = 10;
		int num2 = 20;
		int add = num1 + num2;
	%>
	<!-- %= : 표현, 출력 -->
	<%=num1 %> + <%=num2 %> = <%=add %>
</body>
